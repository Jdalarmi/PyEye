{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI with Uvicorn on Port 8000",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--port",
                "8000"
            ],
            "cwd": "${workspaceFolder}/app/backend"
        }
    ]
}